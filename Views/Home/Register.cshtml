@*Register*@
<section class="material-half-bg ">
    <div class="cover"></div>
</section>
<section class="login-content">
    <div class="logo text-center">
        <img src="~/img/telnet_logo.png" alt="Telent logo" />
        <h1>Device Inventory</h1>
    </div>
    <div class="bg-white">
        <form class="form-horizontal">
            <div class="form-group row">
                <label class="control-label col-md-3">Employee ID</label>
                <div class="col-md-8">
                    <input class="form-control" type="text" placeholder="Enter Employee Id">
                </div>
            </div>
            <div class="form-group row">
                <label class="control-label col-md-3">Fullname</label>
                <div class="col-md-8">
                    <input class="form-control" type="text" placeholder="Enter Fullname">
                </div>
            </div>
            <div class="form-group row">
                <label class="control-label col-md-3">Email</label>
                <div class="col-md-8">
                    <input class="form-control" type="email" placeholder="Enter email address">
                </div>
            </div>
            <div class="form-group row">
                <label class="control-label col-md-3">Subsidiary</label>
                <div class="col-md-8">
                    <select class="form-control">
                        <option value="-1">Select Subsidiary</option>
                    </select>
                </div>
            </div>
          
          
            <div class="form-group row">
                <label class="control-label col-md-3">Profile Image</label>
                <div class="col-md-8">
                    <input class="form-control" type="file">
                </div>
            </div>
            <br />
            <br />
            <br />
            <br />
            <br />
            <br />
            <br />
           
        </form>
   
    </div>
</section>
